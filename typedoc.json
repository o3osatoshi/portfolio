{
  "$schema": "https://typedoc.org/schema.json",
  "categorizeByGroup": false,
  "entryPointStrategy": "packages",
  "entryPoints": [
    "packages/application",
    "packages/auth",
    "packages/config",
    "packages/domain",
    "packages/interface",
    "packages/prisma",
    "packages/toolkit",
    "packages/ui"
  ],
  "exclude": ["**/*.spec.ts", "**/*.stories.tsx", "**/dist/**", "**/.next/**"],
  "excludeExternals": true,
  "excludeInternal": true,
  "excludePrivate": true,
  "excludeProtected": false,
  "gitRevision": "main",
  "hideBreadcrumbs": false,
  "includeVersion": true,
  "out": "docs/api",
  "packageOptions": {
    "entryPoints": ["src/index.ts", "src/index.*.ts"]
  },
  "plugin": ["typedoc-plugin-markdown"],
  "readme": "none",
  "sourceLinkTemplate": "https://github.com/o3osatoshi/portfolio/blob/{gitRevision}/{path}#L{line}",
  "tsconfig": "tsconfig.json"
}
