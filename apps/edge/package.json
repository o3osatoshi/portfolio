{
  "name": "@repo/edge",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "wrangler deploy --dry-run --outdir dist",
    "deploy": "wrangler deploy",
    "deploy:prv": "wrangler versions upload",
    "dev": "wrangler dev",
    "sync:env": "doppler secrets --json --project portfolio-edge --config prd | jq -c 'with_entries(.value = .value.computed)' | wrangler secret bulk"
  },
  "dependencies": {
    "@repo/interface": "workspace:*"
  },
  "devDependencies": {
    "wrangler": "^4.45.3"
  },
  "engines": {
    "node": ">=22"
  }
}
