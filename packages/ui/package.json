{
  "name": "@o3osatoshi/ui",
  "version": "0.0.6",
  "private": false,
  "keywords": [],
  "homepage": "https://github.com/o3osatoshi/portfolio/packages/ui",
  "bugs": {
    "url": "https://github.com/o3osatoshi/portfolio/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/o3osatoshi/portfolio.git",
    "directory": "packages/ui"
  },
  "license": "MIT",
  "sideEffects": [
    "*.css"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js",
      "default": "./dist/client/index.js"
    },
    "./globals.css": "./src/styles/globals.css"
  },
  "files": [
    "dist",
    "src/styles",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "api:extract": "run-s api:extract:server api:extract:client",
    "api:extract:client": "api-extractor run --local -c api-extractor.client.json",
    "api:extract:server": "api-extractor run --local -c api-extractor.server.json",
    "api:report": "run-s api:report:server api:report:client",
    "api:report:client": "api-extractor run --diagnostics -c api-extractor.client.json",
    "api:report:server": "api-extractor run --diagnostics -c api-extractor.server.json",
    "build": "run-p build:server build:client",
    "build:client": "tsup --config tsup.client.config.ts",
    "build:server": "tsup --config tsup.server.config.ts",
    "clean": "rimraf dist",
    "dev": "run-p dev:server dev:client",
    "dev:client": "tsup --config tsup.client.config.ts --watch",
    "dev:server": "tsup --config tsup.server.config.ts --watch",
    "prepublishOnly": "pnpm clean && pnpm typecheck && NODE_ENV=production pnpm build",
    "test": "vitest run",
    "test:cvrg": "vitest run --coverage",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "catalog:",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "catalog:",
    "@o3osatoshi/config": "workspace:*",
    "@storybook/react-vite": "^10.2.3",
    "@storybook/test": "^8.6.15",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@turbo/gen": "^2.8.1",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "jsdom": "^27.4.0",
    "npm-run-all": "catalog:",
    "rimraf": "catalog:",
    "tailwindcss": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vite-tsconfig-paths": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "next": "^14 || ^15",
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "engines": {
    "node": "22.x"
  },
  "publishConfig": {
    "access": "public"
  }
}
