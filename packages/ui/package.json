{
  "name": "@o3osatoshi/ui",
  "version": "0.0.2",
  "private": false,
  "keywords": [],
  "homepage": "https://github.com/o3osatoshi/portfolio/packages/ui",
  "bugs": {
    "url": "https://github.com/o3osatoshi/portfolio/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/o3osatoshi/portfolio.git",
    "directory": "packages/ui"
  },
  "license": "MIT",
  "sideEffects": [
    "*.css"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js",
      "default": "./dist/client/index.js"
    },
    "./globals.css": "./src/styles/globals.css"
  },
  "files": [
    "dist",
    "src/styles",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "run-s build:server build:client",
    "build:client": "tsup --config tsup.client.config.mjs",
    "build:server": "tsup --config tsup.server.config.mjs",
    "clean": "rimraf dist",
    "dev": "run-p dev:server dev:client",
    "dev:client": "tsup --config tsup.client.config.mjs --watch",
    "dev:server": "tsup --config tsup.server.config.mjs --watch",
    "prepublishOnly": "pnpm clean && pnpm typecheck && NODE_ENV=production pnpm build",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.9",
    "@radix-ui/react-collapsible": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.6",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-tooltip": "^1.2.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "catalog:",
    "tailwind-merge": "^3.2.0",
    "tw-animate-css": "^1.2.9"
  },
  "devDependencies": {
    "@o3osatoshi/config": "workspace:*",
    "@turbo/gen": "^2.5.2",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "npm-run-all": "catalog:",
    "rimraf": "catalog:",
    "tailwindcss": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "next": "^14 || ^15",
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "engines": {
    "node": ">=22"
  },
  "publishConfig": {
    "access": "public"
  }
}
