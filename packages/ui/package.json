{
  "name": "@o3osatoshi/ui",
  "version": "0.0.0",
  "private": false,
  "sideEffects": [
    "*.css"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./globals.css": "./src/styles/globals.css",
