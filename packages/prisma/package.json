{
  "name": "@repo/prisma",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "db:push": "npm run with:dev -- prisma db push",
    "db:push:prod": "npm run with:prod -- prisma db push",
    "db:push:prod:force": "npm run with:prod -- prisma db push --accept-data-loss",
    "db:reset": "npm run with:dev -- prisma migrate reset",
    "db:reset:skip-seed": "npm run with:dev -- prisma migrate reset --skip-seed",
    "db:seed": "npm run with:dev -- tsx scripts/seed.ts",
    "db:seed:prod": "npm run with:prod -- tsx scripts/seed.ts",
    "format": "prisma format",
    "generate": "prisma generate",
    "postinstall": "npm run generate",
    "migrate:deploy": "npm run with:prod -- prisma migrate deploy",
    "migrate:dev": "npm run with:dev -- prisma migrate dev",
    "migrate:status": "npm run with:dev -- prisma migrate status",
    "migrate:status:prod": "npm run with:prod -- prisma migrate status",
    "push": "npm run db:push",
    "reset": "npm run db:reset",
    "seed": "npm run db:seed",
    "studio": "npm run with:dev -- prisma studio",
    "studio:prod": "npm run with:prod -- prisma studio",
    "test": "vitest --exclude **/*.int.spec.ts",
    "test:int": "vitest -c vitest.int.config.ts",
    "test:run": "vitest run --exclude **/*.int.spec.ts",
    "test:run:int": "vitest run -c vitest.int.config.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "with:dev": "dotenv -e .env.development.local --",
    "with:prod": "dotenv -e .env.production.local --"
  },
  "dependencies": {
    "@o3osatoshi/toolkit": "workspace:*",
    "@prisma/client": "^6.7.0",
    "@repo/domain": "workspace:*",
    "neverthrow": "catalog:"
  },
  "devDependencies": {
    "@o3osatoshi/config": "workspace:*",
    "@testcontainers/postgresql": "^11.5.1",
    "@types/node": "catalog:",
    "dotenv-cli": "catalog:",
    "prisma": "^6.7.0",
    "testcontainers": "^11.5.1",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "engines": {
    "node": ">=22"
  },
  "prisma": {
    "schema": "./schema"
  }
}
