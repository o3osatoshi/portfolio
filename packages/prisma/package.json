{
  "name": "@repo/prisma",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "format": "prisma format",
    "generate": "prisma generate",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "migrate:reset": "prisma migrate reset",
    "migrate:status": "prisma migrate status",
    "pull:env:dev": "doppler secrets download --project portfolio-prisma --config dev --no-file --format env > ./.env.development.local",
    "pull:env:lcl": "doppler secrets download --project portfolio-prisma --config lcl --no-file --format env > ./.env.test.local",
    "pull:env:prd": "doppler secrets download --project portfolio-prisma --config prd --no-file --format env > ./.env.production.local",
    "studio": "prisma studio",
    "test": "vitest run",
    "test:cvrg": "vitest run --coverage",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@o3osatoshi/toolkit": "workspace:*",
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "@repo/domain": "workspace:*",
    "neverthrow": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@o3osatoshi/config": "workspace:*",
    "@testcontainers/postgresql": "^11.9.0",
    "@types/node": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "npm-run-all": "catalog:",
    "prisma": "^7.1.0",
    "testcontainers": "^11.9.0",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "engines": {
    "node": "22.x"
  }
}
