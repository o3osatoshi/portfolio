{
  "name": "@o3osatoshi/config",
  "version": "1.0.0",
  "private": false,
  "description": "Shared tsup presets and TypeScript tsconfig bases for personal projects.",
  "keywords": [
    "tsup",
    "tsconfig",
    "typescript",
    "build",
    "config"
  ],
  "homepage": "https://github.com/o3osatoshi/portfolio/packages/config",
  "bugs": {
    "url": "https://github.com/o3osatoshi/portfolio/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/o3osatoshi/portfolio.git",
    "directory": "packages/config"
  },
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./biome/*": "./biome/*",
    "./api-extractor/*": "./api-extractor/*",
    "./eslint/*": "./eslint/*",
    "./tsconfig/*": "./tsconfig/*",
    "./tsup": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./vitest": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "types": "./dist/tsup/index.d.ts",
  "files": [
    "dist",
    "tsconfig",
    "biome",
    "eslint",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "api:extract": "api-extractor run --local",
    "api:report": "api-extractor run --diagnostics",
    "build": "tsup",
    "clean": "rimraf dist",
    "dev": "tsup --watch",
    "prepublishOnly": "pnpm clean && pnpm typecheck && NODE_ENV=production pnpm build",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@vitest/browser-playwright": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "rimraf": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "@microsoft/api-extractor": "^7.53.3",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/browser-playwright": "^4.0.15",
    "@vitest/coverage-v8": "^4.0.15",
    "eslint": "^9.35.0",
    "eslint-plugin-jsonc": "^2.20.1",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-vitest": "^0.5.4",
    "jsonc-eslint-parser": "^2.4.0",
    "tsup": "^8.4.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": "22.x"
  },
  "publishConfig": {
    "access": "public"
  }
}
