{
  "name": "@repo/eth",
  "version": "0.0.0",
  "private": true,
  "description": "ETH contract types and hooks (generated)",
  "keywords": [
    "ethereum",
    "eth",
    "web3",
    "wagmi",
    "viem",
    "abi",
    "contracts",
    "hooks",
    "generated"
  ],
  "homepage": "https://github.com/o3osatoshi/portfolio/packages/eth",
  "bugs": {
    "url": "https://github.com/o3osatoshi/portfolio/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/o3osatoshi/portfolio.git",
    "directory": "packages/eth"
  },
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "dev": "tsup --watch",
    "generate": "dotenv -e .env.local -- wagmi generate",
    "prepublishOnly": "pnpm clean && pnpm generate && pnpm typecheck && NODE_ENV=production pnpm build",
    "test": "vitest run",
    "test:cvrg": "vitest run --coverage",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@o3osatoshi/config": "workspace:*",
    "@o3osatoshi/toolkit": "workspace:*",
    "@types/node": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "@wagmi/cli": "^2.9.0",
    "dotenv-cli": "catalog:",
    "rimraf": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "viem": "catalog:",
    "vitest": "catalog:",
    "wagmi": "catalog:",
    "zod": "catalog:"
  },
  "engines": {
    "node": "22.x"
  },
  "publishConfig": {
    "access": "public"
  }
}
